#!/bin/bash
# ===================================================================================================
# Generate Restore Script                Development - Release 1.01                  2015-08-09 20:52
# ===================================================================================================
# Number of Channels per share : 1   
# Number of Mount Points       : 8   
# Number of Instances          : 6   
# Service Name prefix          : TPBP           
# Service Name postfix         : X29U           
# Script File Name             : restore_database                                  
# ===================================================================================================

rman log=restore_database_$$.log<<EOF
connect target /
run {
  sql 'alter system set "_backup_disk_bufcnt"  =      64 scope=memory';
  sql 'alter system set "_backup_disk_bufsz"   = 1048576 scope=memory';
  sql 'alter system set "_backup_file_bufcnt"  =      64 scope=memory';
  sql 'alter system set "_backup_file_bufsz"   = 1048576 scope=memory';

  set newname for database to '+DATA_X29U';
  allocate channel TPBP1_SHARE1_1    DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP1_X29U';
  allocate channel TPBP2_SHARE1_2    DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP2_X29U';
  allocate channel TPBP3_SHARE1_3    DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP3_X29U';
  allocate channel TPBP4_SHARE1_4    DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP4_X29U';
