  allocate channel TPBP4_SHARE8_46   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP4_X29U';
  allocate channel TPBP5_SHARE8_47   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP5_X29U';
  allocate channel TPBP6_SHARE8_48   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP6_X29U';

  # -----------------------------------------------------------------------------------------
  # Uncomment and configure the restore/recover command(s) below as needed for your restore.
  # -----------------------------------------------------------------------------------------
  ### restore database until scn 10862830369582;
  ### restore database until sequence 285487 thread 3;
  ### restore database until time "TO_DATE('20140801232011','YYYYMMDDHH24MISS')";

  ### recover database until scn 10862830369582;
  ### recover database until sequence 285487 thread 3;
  ### recover database until time "TO_DATE('20140801232011','YYYYMMDDHH24MISS')";
