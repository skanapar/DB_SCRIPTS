  allocate channel TPBP6_SHARE1_6    DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP6_X29U';

  allocate channel TPBP1_SHARE2_7    DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP1_X29U';
  allocate channel TPBP2_SHARE2_8    DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP2_X29U';
  allocate channel TPBP3_SHARE2_9    DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP3_X29U';
  allocate channel TPBP4_SHARE2_10   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP4_X29U';
  allocate channel TPBP5_SHARE2_11   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP5_X29U';
  allocate channel TPBP6_SHARE2_12   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP6_X29U';

  allocate channel TPBP1_SHARE3_13   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP1_X29U';
  allocate channel TPBP2_SHARE3_14   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP2_X29U';
  allocate channel TPBP3_SHARE3_15   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP3_X29U';
  allocate channel TPBP4_SHARE3_16   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP4_X29U';
  allocate channel TPBP5_SHARE3_17   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP5_X29U';
  allocate channel TPBP6_SHARE3_18   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP6_X29U';

  allocate channel TPBP1_SHARE4_19   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP1_X29U';
  allocate channel TPBP2_SHARE4_20   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP2_X29U';
  allocate channel TPBP3_SHARE4_21   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP3_X29U';
  allocate channel TPBP4_SHARE4_22   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP4_X29U';
  allocate channel TPBP5_SHARE4_23   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP5_X29U';
  allocate channel TPBP6_SHARE4_24   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP6_X29U';

  allocate channel TPBP1_SHARE5_25   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP1_X29U';
  allocate channel TPBP2_SHARE5_26   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP2_X29U';
  allocate channel TPBP3_SHARE5_27   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP3_X29U';
  allocate channel TPBP4_SHARE5_28   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP4_X29U';
  allocate channel TPBP5_SHARE5_29   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP5_X29U';
  allocate channel TPBP6_SHARE5_30   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP6_X29U';

  allocate channel TPBP1_SHARE6_31   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP1_X29U';
  allocate channel TPBP2_SHARE6_32   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP2_X29U';
  allocate channel TPBP3_SHARE6_33   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP3_X29U';
  allocate channel TPBP4_SHARE6_34   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP4_X29U';
  allocate channel TPBP5_SHARE6_35   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP5_X29U';
  allocate channel TPBP6_SHARE6_36   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP6_X29U';

  allocate channel TPBP1_SHARE7_37   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP1_X29U';
  allocate channel TPBP2_SHARE7_38   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP2_X29U';
  allocate channel TPBP3_SHARE7_39   DEVICE TYPE DISK CONNECT 'sys/Lexus2014@TPBP3_X29U';
