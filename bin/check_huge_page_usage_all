#!/bin/bash

ps -ef | grep pmon | grep -v grep | grep -v perl | grep -v ASM | grep -v DBFS |\
while read PMON; do
  DB=`echo "$PMON" | awk {' print $8 '} | cut -f3 -d_`
  echo
  echo $DB

  MAIN_ENV=~/.env/main.env
  if [ -r $MAIN_ENV ]; then
     . $MAIN_ENV $DB
  else
     echo 'Could not open $MAIN_ENV for read.'
  fi

  grep Huge $_ALERT_LOG_ | tail -3
done

